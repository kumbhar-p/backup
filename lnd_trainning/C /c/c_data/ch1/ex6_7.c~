/* Exercsise 1-6. Verify that the expression getchar() != EOF is 0 or 1.
   
   Exercise 1-7. Write a program to print the value of EOF. */


#include<stdio.h>

int main(void)
{
	int  ch;
	while((ch=getchar())!=EOF)
	{
	putchar(ch);
	printf("%d\n",ch!=EOF);
	}
	printf("\nEOF - %d\n",EOF);
	
	return 0;
}
